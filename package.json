{
    "name": "ghost-gitbook",
    "version": "3.0.0",
    "engines": {
        "ghost": "3.0.0"
    },
    "browserslist": [
        "defaults"
    ],
    "keywords": [
        "ghost-theme"
    ],
    "config": {
        "posts_per_page": 1,
        "image_sizes": {
            "xxs": {
                "width": 30
            },
            "xs": {
                "width": 100
            },
            "s": {
                "width": 300
            },
            "m": {
                "width": 600
            },
            "l": {
                "width": 1000
            },
            "xl": {
                "width": 2000
            }
        }
    },
    "license": "MIT"
}
